{
  "name": "dc-components",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "prepare": "husky install",
    "gulp": "gulp --require @esbuild-kit/cjs-loader -f build/gulpfile.ts",
    "tsc": "vue-tsc -p tsconfig.web.json",
    "build": "pnpm gulp",
    "play": "vite --config play/vite.config.ts dev",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./*": "./*"
  },
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "style": "dist/index.css",
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.4.3",
    "vue": "^3.3.8"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.4.3",
    "vite": "^5.0.4",
    "vue": "^3.3.8"
  },
  "types": "es/index.d.ts",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@pnpm/types": "^9.4.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-clean-css": "^4.3.4",
    "@types/gulp-rename": "^2.0.6",
    "@types/gulp-sass": "^5.0.4",
    "@types/node": "^20.9.3",
    "@types/undertaker": "^1.2.11",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.0",
    "jsdom": "^23.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.1",
    "rollup-plugin-esbuild": "^6.1.0",
    "sass": "^1.69.5",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2",
    "unbuild": "^0.7.4",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": ">= 18",
    "pnpm": ">= 8.11.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
