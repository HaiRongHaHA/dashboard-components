{
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "prepare": "husky install",
    "gen": "bash ./scripts/gc.sh",
    "build": "pnpm run -C packages/dc-build start",
    "demo": "pnpm run -C packages/dc-demo dev"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rolldogking/eslint-config": "workspace:*",
    "@types/node": "^20.9.3",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.55.0",
    "husky": "^8.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 18",
    "pnpm": ">= 8.11.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "Chrome >= 51",
    "iOS >= 10"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "nano-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
